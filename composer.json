{
    "name": "coddin-web/nova4-date-range-filter",
    "description": "A Laravel Nova date range filter.",
    "keywords": [
        "laravel",
        "nova",
        "nova4",
        "date",
        "filter"
    ],
    "version": "1.0.1",
    "license": "MIT",
    "require": {
        "php": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Coddin\\Filters\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Coddin\\Filters\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
